<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="loan_application_tag_list" model="ir.ui.view">
        <field name="name">loan.application.tag.list</field>
        <field name="model">loan.application.tag</field>
        <field name="arch" type="xml">
            <list name="List of Loan Application Tags" default_order="name">
                <field name="name"/>
            </list>
        </field>
    </record>

    <record id="action_loan_tag" model="ir.actions.act_window">
        <field name="name">Loan Tag</field>
        <field name="res_model">loan.application.tag</field>
        <field name="view_mode">list,form</field>
    </record>
</odoo>